<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<%- include("../navBar/navBar", {pageName: `Student's Datas`}) %>
<body>
    <!-- <% console.log(data)%> -->
    <a href="/teachers/add">add teacher</a>
    <table border="1">
        <tr>
            <th>id</th>
            <th>first name</th>
            <th>last name</th>
            <th>email</th>
            <th>gender</th>
            <th>action</th>
        </tr>
        <%data.forEach(el =>{ %>
            <tr>
            <td><%= el.id %></td>
            <td><%= el.first_name %></td>
            <td><%= el.last_name %></td>
            <td><%= el.email %></td>
            <td><%= el.gender %></td>
            <td><a href="/teachers/<%= el.id%>/edit">edit</a>||<a href="/teachers/<%= el.id%>/delete">delete</a></td>

       <% })%>
    </tr>
    </table>
</body>
</html>